<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Incluir en backup en la nube -->
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
        <exclude domain="database" path="tracking_database"/>
    </cloud-backup>
    <device-transfer>
        <!-- Incluir en transferencia de dispositivo -->
        <include domain="sharedpref" path="."/>
        <include domain="file" path="."/>
        <exclude domain="database" path="tracking_database"/>
    </device-transfer>
</data-extraction-rules>